{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "illuminate/html": "^5.0@dev",
        "frozennode/administrator": "5.*",
        "sunhater/kcfinder": "dev-master",
        "efficiently/authority-controller": "dev-master",
        "tsawler/laravel-filemanager": "dev-master",
        
        "suin/php-rss-writer": "dev-master",
        "janusnic/latacomm": "^1.0@dev",
        "mews/captcha": "dev-master",
        "greggilbert/recaptcha": "^2.1@dev",
        "anhskohbo/no-captcha": "dev-master",
        "lucadegasperi/oauth2-server-laravel": "5.0.*@dev",
        "laravel/socialite": "^3.0@dev"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.5",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
